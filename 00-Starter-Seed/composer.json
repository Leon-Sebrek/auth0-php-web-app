{
  "name": "auth0-samples/auth0-php-web-app",
  "description": "Auth0 Integration Samples for PHP Web Applications.",
  "require": {
    "php": "^7.4 || ^8.0",
    "vlucas/phpdotenv": "^5.3",
    "auth0/auth0-php": "8.x-dev",
    "hyperf/event": "^2.1",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.8",
    "http-interop/http-factory-guzzle": "^1.0"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/auth0/auth0-PHP.git"
    }
  ],
  "scripts": {
    "up": "docker compose up --detach --force-recreate",
    "down": "docker compose down --rmi 'all'",
    "start": "@php -S localhost:3000 public/index.php"
  },
  "license": "MIT"
}
